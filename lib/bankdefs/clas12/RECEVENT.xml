<evio_dictionary>
  <bank name="RECEVENT" tag="4500" info="EVNT bank for reconstructed CLAS12 Data">
    <section name="particle"  tag="4501" num="0" info="Container Bank">
      <column name="status"   type="int8"    num="1"   info="status of the particle"/>
      <column name="pid"      type="int32"   num="2"   info="particle ID"/>
      <column name="charge"   type="int8"    num="3"   info="charge of the particle"/>
      <column name="mass"     type="float32" num="4"   info="particle mass calculated from beta"/>
      <column name="beta"     type="float32" num="5"   info="x component of momentum"/>
      <column name="vx"       type="float32" num="6"   info="x component of vertex"/>
      <column name="vy"       type="float32" num="7"   info="y component of vertex"/>
      <column name="vz"       type="float32" num="8"   info="z component of vertex"/>
      <column name="px"       type="float32" num="9"   info="z component of momentum"/>
      <column name="py"       type="float32" num="10"  info="z component of momentum"/>
      <column name="pz"       type="float32" num="11"  info="z component of momentum"/>
      <column name="scpath"   type="float32" num="12"  info="y component of momentum"/>
      <column name="sctime"   type="float32" num="13"  info="y component of momentum"/>
      <column name="sc_x"     type="float32" num="14"  info="y component of momentum"/>
      <column name="sc_y"     type="float32" num="15"  info="y component of momentum"/>
      <column name="sc_z"     type="float32" num="16"  info="y component of momentum"/>
      <column name="sc_error" type="float32" num="17"  info="y component of momentum"/>
      <column name="ecpath"   type="float32" num="18"  info="y component of momentum"/>
      <column name="ectime"   type="float32" num="19"  info="y component of momentum"/>
      <column name="ec_x"     type="float32" num="20"  info="y component of momentum"/>
      <column name="ec_y"     type="float32" num="21"  info="y component of momentum"/>
      <column name="ec_z"     type="float32" num="22"  info="y component of momentum"/>
      <column name="ec_error" type="float32" num="23"  info="y component of momentum"/>
    </section>
  </bank>
</evio_dictionary>
